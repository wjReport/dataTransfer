CREATE TABLE `gl_balances` (
  `SET_OF_BOOKS_ID` int(15) NOT NULL,
  `CODE_COMBINATION_ID` int(15) NOT NULL,
  `CURRENCY_CODE` varchar(15) NOT NULL,
  `PERIOD_NAME` varchar(15) NOT NULL,
  `ACTUAL_FLAG` varchar(1) NOT NULL,
  `LAST_UPDATE_DATE` date NOT NULL,
  `LAST_UPDATED_BY` int(15) NOT NULL,
  `BUDGET_VERSION_ID` int(15) DEFAULT NULL,
  `ENCUMBRANCE_TYPE_ID` int(15) DEFAULT NULL,
  `TRANSLATED_FLAG` varchar(1) DEFAULT NULL,
  `REVALUATION_STATUS` varchar(1) DEFAULT NULL,
  `PERIOD_TYPE` varchar(15) DEFAULT NULL,
  `PERIOD_YEAR` int(15) DEFAULT NULL,
  `PERIOD_NUM` int(15) DEFAULT NULL,
  `PERIOD_NET_DR` decimal(17,2) DEFAULT NULL,
  `PERIOD_NET_CR` decimal(17,2) DEFAULT NULL,
  `PERIOD_TO_DATE_ADB` decimal(17,2) DEFAULT NULL,
  `QUARTER_TO_DATE_DR` decimal(17,2) DEFAULT NULL,
  `QUARTER_TO_DATE_CR` decimal(17,2) DEFAULT NULL,
  `QUARTER_TO_DATE_ADB` decimal(17,2) DEFAULT NULL,
  `YEAR_TO_DATE_ADB` decimal(17,2) DEFAULT NULL,
  `PROJECT_TO_DATE_DR` decimal(17,2) DEFAULT NULL,
  `PROJECT_TO_DATE_CR` decimal(17,2) DEFAULT NULL,
  `PROJECT_TO_DATE_ADB` decimal(17,2) DEFAULT NULL,
  `BEGIN_BALANCE_DR` decimal(17,2) DEFAULT NULL,
  `BEGIN_BALANCE_CR` decimal(17,2) DEFAULT NULL,
  `PERIOD_NET_DR_BEQ` decimal(17,2) DEFAULT NULL,
  `PERIOD_NET_CR_BEQ` decimal(17,2) DEFAULT NULL,
  `BEGIN_BALANCE_DR_BEQ` decimal(17,2) DEFAULT NULL,
  `BEGIN_BALANCE_CR_BEQ` decimal(17,2) DEFAULT NULL,
  `TEMPLATE_ID` int(17) DEFAULT NULL,
  `ENCUMBRANCE_DOC_ID` int(17) DEFAULT NULL,
  `ENCUMBRANCE_LINE_NUM` int(17) DEFAULT NULL,
  `QUARTER_TO_DATE_DR_BEQ` decimal(17,2) DEFAULT NULL,
  `QUARTER_TO_DATE_CR_BEQ` decimal(17,2) DEFAULT NULL,
  `PROJECT_TO_DATE_DR_BEQ` decimal(17,2) DEFAULT NULL,
  `PROJECT_TO_DATE_CR_BEQ` decimal(17,2) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

